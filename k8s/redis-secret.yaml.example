---
# Redis Secret for Sentinel HA
# This secret is referenced by the Redis Helm chart
apiVersion: v1
kind: Secret
metadata:
  name: visage-redis-secret
  namespace: visage
  labels:
    app.kubernetes.io/name: redis
    app.kubernetes.io/part-of: visage
type: Opaque
stringData:
  # Change this password in production!
  password: "visage-redis-ha-password"
